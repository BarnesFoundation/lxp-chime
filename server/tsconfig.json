{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "noEmitOnError": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "../dist/server",
        "baseUrl": "../server",
        "esModuleInterop": true,
        "strict": false,
        "types": []
    },
    "compileOnSave": true,
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "test",
        "../node_modules"
    ]
}